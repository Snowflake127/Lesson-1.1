# "Распаковка позиционных параметров"

print ('1. Функция с параметрами по умолчанию')
def print_params(a = 1, b = 'строка', c = True):
    print(a, b, c)
    print(type(a))
    print(type(b))
    print(type(c))
print_params (c = [1,2,3])
print_params (b = 25)
print_params (b = int(25))
print_params ()
print_params (*(1,False,3))
print_params (1,2,3)
print_params (1,*(False,3))

print ('2. Распаковка параметров')
values_list= ['my dog', 5, True]
values_dict= {'a':'my cat','b': 5, 'c': False}
print_params(*values_list)
print_params(**values_dict)

print('3.Распаковка + отдельные параметры')
values_list_2= ['Galicia', 2024]
print_params(*values_list_2, 42)
